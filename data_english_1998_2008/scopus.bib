@CONFERENCE{Wang2005284,
author={Wang, Y.C. and He, J.J. and Liu, Y.P. and Wu, J. and Lee, C.K. and Haung, Y.T. and Lee, C.K.},
title={Theory and experiment of high voltage step-up ratio disk type piezoelectric transformer for LCD-TV},
journal={Proceedings of the 2005 IEEE International Conference on Mechatronics, ICM '05},
